<Interfaces xmlns="urn:msgplus:interface" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="urn:msgplus:interface PlusInterface.xsd">

<Window Id="WndNew" Version="1">

	<Attributes>
		<Caption>Plus! Poll  |  New poll...</Caption>
	</Attributes>

	<TitleBar>
		<AllowMinimize>false</AllowMinimize>
		<Title>
			<Prefix>Image</Prefix>
			<Text>Plus! Poll  |  New poll...</Text>
		</Title>
	</TitleBar>

	<Position Width="232" Height="187">
		<IsAbsolute>true</IsAbsolute>
	</Position>

	<DialogTmpl/>

	<Controls>
		<Control xsi:type="StaticControl" Id="TxtQuestion">
			<Position Left="3" Top="3" Width="48">
				<Units>AllPixels</Units>
			</Position>
			<Attributes>
				<Align>Right</Align>
			</Attributes>
			<Caption>Question:</Caption>
		</Control>
		<Control xsi:type="EditControl" Id="EdtQuestion">
			<Position Left="57" Top="0" Width="158">
				<Units>AllPixels</Units>
			</Position>
			<Help>Enter your question.</Help>
		</Control>
		<Control xsi:type="StaticControl" Id="TxtAnswer">
			<Position Left="3" Top="25" Width="48">
				<Units>AllPixels</Units>
			</Position>
			<Attributes>
				<Align>Right</Align>
			</Attributes>
			<Caption>Answer:</Caption>
		</Control>
		<Control xsi:type="ComboBoxControl" Id="CmbAnswer">
			<Position Left="57" Top="22" Width="158">
				<Units>AllPixels</Units>
			</Position>
			<Help>Select the answer type.</Help>
		</Control>
		<Control xsi:type="StaticControl" Id="TxtChoices" Enabled="False">
			<Position Left="3" Top="47" Width="48">
				<Units>AllPixels</Units>
			</Position>
			<Attributes>
				<Align>Right</Align>
			</Attributes>
			<Caption>Choices:</Caption>
		</Control>
		<Control xsi:type="EditControl" Id="EdtChoices" Enabled="False">
			<Position Left="57" Top="44" Width="158">
				<Units>AllPixels</Units>
			</Position>
			<Help>Enter the possible answers to your question, separated by pipe '|' characters.\nNote that there must be at least two options entered.</Help>
		</Control>
		<Control xsi:type="StaticControl" Id="TxtContacts">
			<Position Left="3" Top="69" Width="48">
				<Units>AllPixels</Units>
			</Position>
			<Caption>Contacts:</Caption>
		</Control>
		<Control xsi:type="ComboBoxControl" Id="CmbContacts">
			<Position Left="57" Top="66" Width="158">
				<Units>AllPixels</Units>
			</Position>
			<Help>Select the contacts that will receive the poll.</Help>
		</Control>
		<Control xsi:type="StaticControl" Id="TxtVoters" Enabled="False">
			<Position Left="3" Top="91" Width="48">
				<Units>AllPixels</Units>
			</Position>
			<Attributes>
				<Align>Right</Align>
			</Attributes>
			<Caption>Voters:</Caption>
		</Control>
		<Control xsi:type="EditControl" Id="EdtVoters" Enabled="False">
			<Position Left="57" Top="88" Width="158">
				<Units>AllPixels</Units>
			</Position>
			<Help>Enter individual contact email addresses, separated by pipe '|' characters.</Help>
		</Control>
		<Control xsi:type="ButtonControl" Id="BtnPoll" Enabled="False">
			<Position Left="3" Top="115" Width="90">
				<Units>AllPixels</Units>
			</Position>
			<Attributes>
				<IsDefault>true</IsDefault>
			</Attributes>
			<StandardLook Template="Blue"/>
			<Caption>&amp;Start the poll!</Caption>
			<Help>Click to start your poll...</Help>
		</Control>
		<Control xsi:type="ButtonControl" Id="BtnCancel">
			<Position Left="165" Top="115" Width="50">
				<Units>AllPixels</Units>
			</Position>
			<Caption>&amp;Cancel</Caption>
			<Help>Quit the poll creation...</Help>
		</Control>
	</Controls>

</Window>


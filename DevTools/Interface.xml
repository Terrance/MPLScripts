<Window Id="WndTools" Version="1">

	<Attributes>
		<Caption>DevTools</Caption>
		<TopMost>true</TopMost>
	</Attributes>

	<TitleBar>
		<Title>
			<Prefix>Image</Prefix>
			<Text>DevTools</Text>
		</Title>
	</TitleBar>

	<Position Width="179" Height="72">
		<IsAbsolute>true</IsAbsolute>
	</Position>

	<DialogTmpl/>

	<Controls>
		<Control xsi:type="ComboBoxControl" Id="CmbSelect">
			<Position Left="2" Top="-3" Width="131">
				<Units>AllPixels</Units>
				<Anchor Horizontal="LeftRightFixed"/>
			</Position>
		</Control>
		<Control xsi:type="ButtonControl" Id="BtnAbout" Enabled="False">
			<Position Left="138" Top="-6" Width="25">
				<Units>AllPixels</Units>
				<Anchor Horizontal="RightFixed"/>
			</Position>
			<Image>
				<Name>icon-info</Name>
			</Image>
			<Help>View information about this tool.</Help>
		</Control>
	</Controls>

	<Elements>
		<Element xsi:type="LineElement" Id="LneTop">
			<Position Left="2" Top="23" Width="160" Height="0">
				<Units>AllPixels</Units>
				<Anchor Horizontal="LeftRightFixed"/>
			</Position>
			<Color>
				<BaseColor/>
			</Color>
		</Element>
		<Element xsi:type="FigureElement" Id="FgrBox">
			<Position Left="2" Top="23" Width="160">
				<Units>AllPixels</Units>
				<Anchor Horizontal="LeftRightFixed" Vertical="TopBottomFixed"/>
			</Position>
			<Figure>
				<Rectangle/>
			</Figure>
			<Border>
				<Color>
					<BaseColor/>
				</Color>
			</Border>
		</Element>
	</Elements>

</Window>

<Window Id="WndInstall" Version="1">

	<Attributes>
		<Caption>Install / DevTools</Caption>
	</Attributes>

	<TitleBar>
		<AllowMaximize>true</AllowMaximize>
		<Title>
			<Prefix>Image</Prefix>
			<Text>Install / DevTools</Text>
		</Title>
	</TitleBar>

	<Position Width="400" Height="300">
		<IsAbsolute>true</IsAbsolute>
		<Resizeable Allowed="BothSides">
			<MinWidth>400</MinWidth>
			<MinHeight>300</MinHeight>
		</Resizeable>
	</Position>

	<DialogTmpl/>

	<Controls>
		<Control xsi:type="StaticControl" Id="TxtID">
			<Position Left="0" Top="0" Width="0">
				<Units>AllPixels</Units>
			</Position>
		</Control>
		<Control xsi:type="StaticControl" Id="TxtInstall">
			<Position Left="3" Top="-3" Width="380" Height="14">
				<Units>AllPixels</Units>
			</Position>
			<Attributes>
				<AutoAdjustWidth>true</AutoAdjustWidth>
			</Attributes>
			<Caption>Are you sure you want to install this tool?</Caption>
		</Control>
		<Control xsi:type="ButtonControl" Id="BtnInstall">
			<Position Left="219" Top="-13" Width="80">
				<Units>AllPixels</Units>
				<Anchor Horizontal="RightFixed"/>
			</Position>
			<Image>
				<Name>sounds-check</Name>
			</Image>
			<Caption>Install</Caption>
		</Control>
		<Control xsi:type="ButtonControl" Id="BtnCancel">
			<Position Left="304" Top="-13" Width="80">
				<Units>AllPixels</Units>
				<Anchor Horizontal="RightFixed"/>
			</Position>
			<Image>
				<Name>icon-delete</Name>
			</Image>
			<Caption>Cancel</Caption>
		</Control>
		<Control xsi:type="RichEditControl" Id="EdtCode">
			<Position Left="2" Top="16" Width="382" Height="236">
				<Units>AllPixels</Units>
				<Anchor Horizontal="LeftRightFixed" Vertical="TopBottomFixed"/>
			</Position>
			<Attributes>
				<ReadOnly>true</ReadOnly>
			</Attributes>
		</Control>
	</Controls>

</Window>

